{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"lib": [
			"esnext",
			"esnext.asynciterable",
			"dom"
		],
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"allowJs": false,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true,
		"alwaysStrict": true,
		"importHelpers": true,
		"pretty": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"baseUrl": ".",
		// "paths": {
		// 	"~/*": [
		// 		"./*"
		// 	],
		// 	"@/*": [
		// 		"./*"
		// 	],
		// 	"~~/*": [
		// 		"./*"
		// 	],
		// 	"@@/*": [
		// 		"./*"
		// 	]
		// },
		// "types": ["@types/node", "@nuxt/types", "@nuxtjs/color-mode", "@nuxtjs/axios"]
		"types": [
			"nuxt-windicss"
		]
	},
	"exclude": [
		"node_modules",
		".nuxt",
		".output",
		"backup"
	],
}
